<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Build your Console Applications fluently faster.</Description>
    <PackageTags>Console;Fluent;Consolefy;</PackageTags>
    <Authors>Erick Bressani</Authors>
    <Company></Company>
    <Product>Consolefy</Product>
    <PackageId>Consolefy</PackageId>
    <RepositoryUrl>https://github.com/erickbressani/Consolefy</RepositoryUrl>
    <PackageProjectUrl>https://github.com/erickbressani/Consolefy</PackageProjectUrl>
    <Version>1.5.0</Version>
    <PackageReleaseNotes>+ Added more options to If's methods
+ Added Quit method
+ Added WaitAnyKeyFromUser method</PackageReleaseNotes>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <AssemblyVersion>1.5.0.0</AssemblyVersion>
    <FileVersion>1.5.0.0</FileVersion>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
